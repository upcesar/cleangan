<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://cleangap.westcentralus.cloudapp.azure.com/api</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>cleangap.westcentralus.cloudapp.azure.com:8172</MSDeployServiceURL>
    <DeployIisAppPath>cleangap/api</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>CLEANGAP-WEB\cleangap</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="cleangap" Order="1" Enabled="True">
          <Destination Path="Data Source=cleangap-sqlserver.database.windows.net;Initial Catalog=cleangap;Persist Security Info=True;User ID=cleangap;Password=cl3@ngapSQL" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="cleangap.api.DAL.CleanGapDataContext, cleangap.api" MigrationConfiguration="cleangap.api.Migrations.Configuration, cleangap.api" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)cleangap-Web.config Connection String">
      <ParameterValue>Data Source=cleangap-sqlserver.database.windows.net;Initial Catalog=cleangap;Persist Security Info=True;User ID=cleangap;Password=cl3@ngapSQL</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>