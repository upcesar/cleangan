<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://cleangap.westcentralus.cloudapp.azure.com:8080/</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>cleangap.westcentralus.cloudapp.azure.com:8172</MSDeployServiceURL>
    <DeployIisAppPath>cleangap_api</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>CLEANGAP-WEB\cleangap</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="cleangap_DatabasePublish" Order="2" Enabled="False">
          <Destination Path="Data Source=cleangap-sqlserver.database.windows.net;Initial Catalog=cleangap;Persist Security Info=True;User ID=cleangap;Password=cl3@ngapSQL" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=BETADEV-WIN7\SQLEXPRESS;Initial Catalog=cleangap;Integrated Security=True;Application Name=EntityFramework" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\cleangap_DatabasePublish_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="data source=BETADEV-WIN7\SQLEXPRESS;initial catalog=cleangap;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)cleangap_DatabasePublish-Web.config Connection String">
      <ParameterValue>data source=cleangap-sqlserver.database.windows.net;initial catalog=cleangap;persist security info=True;user id=cleangap;password=cl3@ngapSQL;MultipleActiveResultSets=True;App=EntityFramework</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>